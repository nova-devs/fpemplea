<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Threadmetadata" table="threadmetadata">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="isDeleted" type="boolean" column="is_deleted" nullable="false"/>
    <field name="lastParticipantMessageDate" type="datetime" column="last_participant_message_date" nullable="true"/>
    <field name="lastMessageDate" type="datetime" column="last_message_date" nullable="true"/>
  </entity>
</doctrine-mapping>
